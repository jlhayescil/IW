                <td><span data-bind="text: state"></span></td>
                <td><span data-bind="text: pc"></span></td>
                <td><span data-bind="text: shipMethod"></span></td>
                <td><span data-bind="text: subTotal"></span></td>
                <td><span data-bind="text: tax"></span></td>
                <td><span data-bind="text: freight"></span></td>
                <td><span data-bind="text: total"></span></td>*@
            </tr>
        </tbody>
    </table>
</div>


